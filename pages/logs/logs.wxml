<!--logs.wxml-->
<van-collapse value="{{ activeNames }}" bind:change="onChange">
  <van-collapse-item name="1">
    <view slot="title">已接受任务<van-icon name="question-o" /></view>
    <view wx:for="{{unfinishedArray}}" wx:key="item">
      <van-swipe-cell id="swipe-cell" right-width="{{ 65 }}" async-close bind:close="onClose">
        <view bindtap='viewDetail' data-desc='{{item}}'>
          <view class="weui-media-box weui-media-box_appmsg">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image class="weui-media-box__thumb" src="../../source/image/订单.png" />
            </view>
            <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title">简介： {{item.desc}}</view>
              <view class="weui-media-box__desc">报酬： {{item.money}}</view>
            </view>
          </view>
        </view>
        <view slot="right" data-desc='{{item}}' bindtap='deleteTask'>删除</view>
      </van-swipe-cell>
      <view class='divLine'></view>
    </view>
  </van-collapse-item>
  <van-collapse-item name="2">
    <view slot="title">已完成任务<van-icon name="question-o" /></view>
    <view wx:for="{{finished}}" wx:key="item">
      <van-swipe-cell id="swipe-cell" right-width="{{ 65 }}" async-close bind:close="onClose">
        <view bindtap='viewDetail' data-desc='{{item}}'>
          <view class="weui-media-box weui-media-box_appmsg">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image class="weui-media-box__thumb" src="../../source/image/订单.png" />
            </view>
            <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title">简介： {{item.desc}}</view>
              <view class="weui-media-box__desc">报酬： {{item.money}}</view>
            </view>
          </view>
        </view>
        <view slot="right" data-desc='{{item}}' bindtap='deleteTask'>删除</view>
      </van-swipe-cell>
      <view class='divLine'></view>
    </view>
  </van-collapse-item>
  <van-collapse-item name="3">
    <view slot="title">已发布任务<van-icon name="question-o" /></view>
    <view wx:for="{{publishedArray}}" wx:key="item">
      <van-swipe-cell id="swipe-cell" right-width="{{ 65 }}" async-close bind:close="onClose">
        <view bindtap='viewDetail' data-desc='{{item}}'>
          <view class="weui-media-box weui-media-box_appmsg">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image class="weui-media-box__thumb" src="../../source/image/订单.png" />
            </view>
            <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title">简介： {{item.desc}}</view>
              <view class="weui-media-box__desc">报酬： {{item.money}}</view>
            </view>
          </view>
        </view>
        <view slot="right" data-desc='{{item}}' bindtap='deleteTask'>删除</view>
      </van-swipe-cell>
      <view class='divLine'></view>
    </view>
  </van-collapse-item>
</van-collapse>
<van-toast id="van-toast" />



